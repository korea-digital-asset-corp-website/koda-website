    "ts-jest": "^25.2.0",
    "typescript": "^5.4.3"
  },
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10",
    "IE 11"
  ],
  "license": "MIT",
  "author": "BoBeenLee <globaldev@naver.com>",
  "storybook-deployer": {
    "gitUsername": "GH Pages Bot",
    "gitEmail": "globaldev@naver.com",
    "commitMessage": "Deploy Storybook to GitHub Pages"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run prepush"
    }
  }
}
